<h2>Zuj/Orp import</h2>

<div class="well">
    Nahrajte xls/xlsx soubor pro import ZÚJ a ORP s pořadím sloupců [Název ZÚJ, KÓD ZÚJ, Název ORP, Kód ORP] bez hlavičky
</div>

@using (Html.BeginForm("ZujOrp", "Import", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    
    <div class="form-group">
        <input type="File" name="file" id="file" value="Choose File" />
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-default" />
    </div>
}
